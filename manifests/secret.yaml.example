# Example Secret template - DO NOT commit real credentials!
# Copy this file to secret.yaml and replace with actual values
# Add secret.yaml to .gitignore to prevent accidental commits

apiVersion: v1
kind: Secret
metadata:
  name: xui-admin-secret
  namespace: xui-vpn
  labels:
    app: 3x-ui
stringData:
  XUI_ADMIN_USER: "<YOUR_ADMIN_USERNAME>"
  XUI_ADMIN_PASS: "<YOUR_SECURE_PASSWORD>"
